    {
      "command": {
        "action": "wt",
        "commandline": "new-tab pwsh.exe ; split-pane -p \"Command Prompt\" -d C:\\ ; split-pane -p \"Ubuntu\" -H"
      },
      "keys": "ctrl+a"
    },
    // open settings ui
    { "command": { "action": "openSettings", "target": "settingsUI" }, "keys": "ctrl+shift+," },
