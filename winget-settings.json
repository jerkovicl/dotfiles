{
  // For documentation on these settings, see: https://aka.ms/winget-settings
  // Gui tool - https://winstall.app/
  // join insider program for experimental builds
  // Winget Powershell module - Install-Module -Name Microsoft.WinGet.Client
  // winget repair - repair the winget installation for page
  // --allow-reboot flag - allow reboot after installation
  // winget install docs - https://github.com/KK-Designs/winget-cli/blob/master/doc/windows/package-manager/winget/install.md
  "$schema": "https://aka.ms/winget-settings.schema.json",
  "source": {
    "autoUpdateIntervalInMinutes": 5
  },
  "visual": {
    "progressBar": "rainbow", // accent (default), retro, rainbow,
    "anonymizeDisplayedPaths": true
  },
  "experimentalFeatures": {
    "experimentalCmd": true,
    "experimentalArg": true,
    "dependencies": true,
    "directMSI": true,
    "experimentalMSStore": true,
    "uninstallPreviousArgument": true,
    "pinning": true,
    "windowsFeature": true,
    "configuration": true,
    "zipInstall": true,
    "list": true,
    "upgrade": true,
    "uninstall": true,
    "export": true,
    "resume": true,
    "configuration03": true,
    "reboot": true,
    "sideBySide": true,
    "configureSelfElevate": true,
    "storeDownload": true,
    "configureExport": true,
    "indexV2": true
  },
  "installBehavior": {
    "preferences": {
      "architectures": ["x64", "x86"]
    }
  },
  "telemetry": {
    "disable": true
  }
}
