{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
  "console_title": true,
  "final_space": true,
  "console_title_style": "template", // folder
  "console_title_template": "{{.Path}} : {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "horizontal_offset": 0,
      "vertical_offset": 0,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#26C6DA",
          "background": "#546E7A",
          "leading_diamond": "î‚¶",
          "trailing_diamond": "î‚°",
          "properties": {
            "prefix": "ðŸš€ðŸ”¥",
            "postfix": "",
            "macos": "mac"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#c386f1",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_host": false
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#ff479c",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "prefix": " î—¿ ",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#193549",
          "background": "#fffb38",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "prefix": " \uE718 "
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_version": true,
            "prefix": " îœŒ "
          }
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#1BD760",
          "properties": {
            "prefix": "\uF9C6 ",
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D ",
            "track_separator": " - "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#ffff66",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": null
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "properties": {
            "battery_icon": "",
            "discharging_icon": "\uE231 ",
            "charging_icon": "\uE234 ",
            "charged_icon": "\uE22F ",
            "color_background": true,
            "charged_color": "#4caf50",
            "charging_color": "#40c4ff",
            "discharging_color": "#ff5722",
            "postfix": "\uF295 ",
            "display_charging": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "î‚´",
          "properties": {
            "always_enabled": true,
            "color_background": true,
            "display_exit_code": false,
            "error_color": "#f1184c",
            "prefix": "<transparent>î‚°</> îˆº"
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#43D426",
          "properties": {
            "prefix": "",
            "postfix": " ",
            "text": "\u276F"
          }
        }
      ]
    }
  ]
}
