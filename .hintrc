{
  "extends": ["web-recommended"],
  "formatters": ["html"],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "warning"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "warning"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "warning"
      }
    ],
    "html-checker": [
      "off",
      {
        "details": true
      }
    ],
    "typescript-config/is-valid": "warning"
  },
  "parsers": [
    "babel-config",
    "css",
    "html",
    "javascript",
    "manifest",
    "typescript-config",
    "webpack-config"
  ]
}
